{
  "name": "revocal",
  "version": "0.0.1",
  "description": "discourdez",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm install && ts-node index.ts",
    "dev": "nodemon index.ts",
    "client": "npm start --prefix client",
    "watch": "concurrently \"npm run dev\" \"npm run client\" ",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./migrations/discourde -d ./db/AppDataSource.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./db/AppDataSource.ts",
    "tsc": "tsc"
  },
  "proxy": "http://localhost:5001",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "discourde": "file:..",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.16",
    "peer": "^0.6.1",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.0",
    "socket.io": "^4.5.1",
    "ts-node": "^10.8.1",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "concurrently": "^7.2.2"
  }
}
